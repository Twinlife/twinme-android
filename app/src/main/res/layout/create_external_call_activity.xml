<?xml version="1.0" encoding="utf-8"?>
<androidx.percentlayout.widget.PercentRelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/create_external_call_activity_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/create_external_call_activity_avatar_view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:scaleType="centerCrop"
        android:layout_centerHorizontal="true" />

    <ImageView
        android:id="@+id/create_external_call_activity_no_avatar_view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:visibility="gone"
        android:scaleType="fitCenter"
        android:src="@drawable/edit_avatar"
        android:layout_centerHorizontal="true"
        app:layout_marginTopPercent="14.99%"
        app:layout_aspectRatio="107%"
        app:layout_widthPercent="20%" />

    <androidx.percentlayout.widget.PercentRelativeLayout
        android:id="@+id/create_external_call_activity_content_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/show_contact_content">

        <View
            android:id="@+id/create_external_call_activity_slide_mark_view"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            app:layout_widthPercent="12%" />

        <androidx.percentlayout.widget.PercentRelativeLayout
            android:id="@+id/create_external_call_activity_content_header_view"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:id="@+id/create_external_call_activity_title_view"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:gravity="center"
                android:lines="1"
                android:text="@string/premium_services_activity_click_to_call_title"
                app:layout_widthPercent="56%" />

        </androidx.percentlayout.widget.PercentRelativeLayout>

        <androidx.percentlayout.widget.PercentRelativeLayout
            android:id="@+id/create_external_call_activity_name_content_view"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_below="@+id/create_external_call_activity_content_header_view">

            <EditText
                android:id="@+id/create_external_call_activity_name_view"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_gravity="left"
                android:background="@android:color/transparent"
                android:hint="@string/create_external_call_activity_placeholder"
                android:imeOptions="actionDone"
                android:inputType="text|textNoSuggestions|textCapSentences"
                app:layout_marginStartPercent="5.31%"
                app:layout_marginEndPercent="5.31%" />

        </androidx.percentlayout.widget.PercentRelativeLayout>

        <TextView
            android:id="@+id/create_external_call_activity_counter_name_view"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/create_external_call_activity_name_content_view"
            android:layout_alignEnd="@+id/create_external_call_activity_name_content_view"
            android:layout_alignRight="@+id/create_external_call_activity_name_content_view" />

        <androidx.percentlayout.widget.PercentRelativeLayout
            android:id="@+id/create_external_call_activity_description_content_view"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_below="@+id/create_external_call_activity_name_content_view">

            <EditText
                android:id="@+id/create_external_call_activity_description_view"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:hint="@string/navigation_activity_about_twinme"
                android:background="@android:color/transparent"
                android:gravity="top"
                android:inputType="textMultiLine|textNoSuggestions|textCapSentences"
                app:layout_marginStartPercent="5.31%"
                app:layout_marginEndPercent="5.31%"
                app:layout_marginTopPercent="4.92%"
                app:layout_marginBottomPercent="4.92%" />
        </androidx.percentlayout.widget.PercentRelativeLayout>

        <TextView
            android:id="@+id/create_external_call_activity_counter_description_view"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/create_external_call_activity_description_content_view"
            android:layout_alignEnd="@+id/create_external_call_activity_description_content_view"
            android:layout_alignRight="@+id/create_external_call_activity_description_content_view" />

        <androidx.percentlayout.widget.PercentRelativeLayout
            android:id="@+id/create_external_call_activity_settings_view"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_below="@+id/create_external_call_activity_description_content_view">

            <TextView
                android:id="@+id/create_external_call_activity_settings_text_view"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:text="@string/show_call_activity_settings_call"
                app:layout_marginEndPercent="10.27%" />

            <ImageView
                android:id="@+id/create_external_call_activity_settings_accessory_view"
                android:layout_centerVertical="true"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:scaleType="fitCenter"
                android:src="@drawable/select_value_icon"
                app:layout_aspectRatio="62%"
                app:layout_heightPercent="34%" />
        </androidx.percentlayout.widget.PercentRelativeLayout>

        <androidx.percentlayout.widget.PercentRelativeLayout
            android:id="@+id/create_external_call_activity_limited_view"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_below="@+id/create_external_call_activity_settings_view">

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="?attr/separator_color" />

            <org.twinlife.twinme.utils.SwitchView
                android:id="@+id/create_external_call_activity_limited_checkbox"
                style="@style/textAppearanceSmall"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:checked="true"
                android:text="@string/show_call_activity_settings_limited"
                android:textOn=""
                android:textOff=""
                android:track="@drawable/switch_track"
                android:thumb="@drawable/switch_thumb_on"
                android:switchMinWidth="0dp" />
        </androidx.percentlayout.widget.PercentRelativeLayout>

        <androidx.percentlayout.widget.PercentRelativeLayout
            android:id="@+id/create_external_call_activity_start_view"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_below="@+id/create_external_call_activity_limited_view">

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="?attr/separator_color" />

            <TextView
                android:id="@+id/create_external_call_activity_start_text_view"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:text="@string/show_call_activity_settings_start"
                app:layout_marginEndPercent="10.27%" />

            <androidx.percentlayout.widget.PercentRelativeLayout
                android:id="@+id/create_external_call_activity_start_date_view"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                app:layout_heightPercent="66.67%"
                android:layout_toLeftOf="@+id/create_external_call_activity_start_hour_view"
                android:layout_toStartOf="@+id/create_external_call_activity_start_hour_view">

                <TextView
                    android:id="@+id/create_external_call_activity_start_date_text_view"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_centerInParent="true"
                    android:lines="1"
                    android:gravity="center" />

            </androidx.percentlayout.widget.PercentRelativeLayout>

            <androidx.percentlayout.widget.PercentRelativeLayout
                android:id="@+id/create_external_call_activity_start_hour_view"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true"
                app:layout_heightPercent="66.67%">

                <TextView
                    android:id="@+id/create_external_call_activity_start_hour_text_view"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_centerInParent="true"
                    android:gravity="center"
                    android:lines="1"
                    android:text="" />

            </androidx.percentlayout.widget.PercentRelativeLayout>

        </androidx.percentlayout.widget.PercentRelativeLayout>

        <androidx.percentlayout.widget.PercentRelativeLayout
            android:id="@+id/create_external_call_activity_end_view"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_below="@+id/create_external_call_activity_start_view">

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="?attr/separator_color" />

            <TextView
                android:id="@+id/create_external_call_activity_end_text_view"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:text="@string/show_call_activity_settings_end"
                app:layout_marginEndPercent="4.27%" />

            <androidx.percentlayout.widget.PercentRelativeLayout
                android:id="@+id/create_external_call_activity_end_date_view"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                app:layout_heightPercent="66.67%"
                android:layout_toLeftOf="@+id/create_external_call_activity_end_hour_view"
                android:layout_toStartOf="@+id/create_external_call_activity_end_hour_view">

                <TextView
                    android:id="@+id/create_external_call_activity_end_date_text_view"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_centerInParent="true"
                    android:gravity="center"
                    android:lines="1"
                    android:text="" />

            </androidx.percentlayout.widget.PercentRelativeLayout>

            <androidx.percentlayout.widget.PercentRelativeLayout
                android:id="@+id/create_external_call_activity_end_hour_view"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true"
                app:layout_heightPercent="66.67%">

                <TextView
                    android:id="@+id/create_external_call_activity_end_hour_text_view"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_centerInParent="true"
                    android:gravity="center"
                    android:lines="1"
                    android:text="" />

            </androidx.percentlayout.widget.PercentRelativeLayout>

        </androidx.percentlayout.widget.PercentRelativeLayout>

        <androidx.percentlayout.widget.PercentRelativeLayout
            android:id="@+id/create_external_call_activity_save_view"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_centerHorizontal="true"
            android:layout_below="@+id/create_external_call_activity_end_view">

            <TextView
                android:id="@+id/create_external_call_activity_save_title_view"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_centerInParent="true"
                android:gravity="center"
                android:text="@string/application_save"
                app:layout_widthPercent="89.06%" />

        </androidx.percentlayout.widget.PercentRelativeLayout>

        <TextView
            android:id="@+id/create_external_call_activity_message_view"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_below="@+id/create_external_call_activity_save_view"
            android:gravity="center"
            android:text="@string/create_external_call_activity_message" />

    </androidx.percentlayout.widget.PercentRelativeLayout>

    <androidx.percentlayout.widget.PercentRelativeLayout
        android:id="@+id/create_external_call_activity_back_clickable_view"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:clickable="true"
        android:orientation="horizontal"
        app:layout_aspectRatio="100%">

        <org.twinlife.twinme.utils.RoundedView
            android:id="@+id/create_external_call_activity_back_rounded_view"
            android:layout_width="match_parent"
            android:layout_height="match_parent" />

        <ImageView
            android:id="@+id/create_external_call_activity_back_image_view"
            android:layout_centerInParent="true"
            android:scaleType="fitCenter"
            android:src="@drawable/start_chevron"
            app:layout_aspectRatio="62.5%"
            app:layout_heightPercent="37.5%" />

    </androidx.percentlayout.widget.PercentRelativeLayout>

    <View
        android:id="@+id/create_external_call_activity_overlay_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:clickable="true"
        android:visibility="invisible" />

    <org.twinlife.twinme.ui.externalCallActivity.MenuCallCapabilitiesView
        android:id="@+id/create_external_call_activity_menu_call_capabilities_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

    <ProgressBar
        android:id="@+id/create_external_call_activity_progress_bar"
        style="@android:style/Widget.ProgressBar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true"
        android:visibility="gone" />

</androidx.percentlayout.widget.PercentRelativeLayout>
