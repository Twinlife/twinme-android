<?xml version="1.0" encoding="utf-8"?>
<androidx.percentlayout.widget.PercentRelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <View
        android:id="@+id/call_quality_view_overlay_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:clickable="true"
        android:visibility="visible" />

    <androidx.percentlayout.widget.PercentRelativeLayout
        android:id="@+id/call_quality_view_action_view"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true">

        <View
            android:id="@+id/call_quality_view_slide_mark_view"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            app:layout_widthPercent="12%" />

        <androidx.percentlayout.widget.PercentRelativeLayout
            android:id="@+id/call_quality_view_stars_view"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/call_quality_view_slide_mark_view"
            android:layout_centerHorizontal="true">

            <ImageView
                android:id="@+id/call_quality_view_star_one_image_view"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:scaleType="fitCenter"
                android:src="@drawable/star_grey"/>

            <ImageView
                android:id="@+id/call_quality_view_star_two_image_view"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:scaleType="fitCenter"
                android:layout_toEndOf="@+id/call_quality_view_star_one_image_view"
                android:src="@drawable/star_grey"/>

            <ImageView
                android:id="@+id/call_quality_view_star_three_image_view"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:scaleType="fitCenter"
                android:layout_toEndOf="@+id/call_quality_view_star_two_image_view"
                android:src="@drawable/star_grey"/>

            <ImageView
                android:id="@+id/call_quality_view_star_four_image_view"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:scaleType="fitCenter"
                android:layout_toEndOf="@+id/call_quality_view_star_three_image_view"
                android:src="@drawable/star_grey"/>
        </androidx.percentlayout.widget.PercentRelativeLayout>

        <TextView
            android:id="@+id/call_quality_view_title_view"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_below="@+id/call_quality_view_stars_view"
            android:gravity="center"
            android:text="@string/call_activity_quality_title"
            app:layout_widthPercent="94%" />

        <TextView
            android:id="@+id/call_quality_view_message_view"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_below="@+id/call_quality_view_title_view"
            android:gravity="center"
            android:text="@string/call_activity_quality_message"
            app:layout_widthPercent="94%" />

        <androidx.percentlayout.widget.PercentRelativeLayout
            android:id="@+id/call_quality_view_confirm_view"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_below="@+id/call_quality_view_message_view">

            <TextView
                android:id="@+id/call_quality_view_confirm_text_view"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:gravity="center"
                android:text="@string/feedback_activity_send" />

        </androidx.percentlayout.widget.PercentRelativeLayout>

        <androidx.percentlayout.widget.PercentRelativeLayout
            android:id="@+id/call_quality_view_cancel_view"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_below="@+id/call_quality_view_confirm_view">

            <TextView
                android:id="@+id/call_quality_view_cancel_text_view"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:text="@string/application_cancel" />
        </androidx.percentlayout.widget.PercentRelativeLayout>

    </androidx.percentlayout.widget.PercentRelativeLayout>

</androidx.percentlayout.widget.PercentRelativeLayout>