<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/message_settings_space_activity_content_view"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <include
        android:id="@+id/message_settings_space_activity_tool_bar"
        layout="@layout/main_toolbar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/message_settings_space_activity_list_view"
        android:layout_below="@+id/message_settings_space_activity_tool_bar"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

    <View
        android:id="@+id/message_settings_space_activity_overlay_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:clickable="true"
        android:visibility="invisible" />

    <org.twinlife.twinme.ui.settingsActivity.MenuSelectValueView
        android:id="@+id/message_settings_space_activity_menu_timeout_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

</RelativeLayout>