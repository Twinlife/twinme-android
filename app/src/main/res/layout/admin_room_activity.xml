<?xml version="1.0" encoding="utf-8"?>
<androidx.percentlayout.widget.PercentRelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/admin_room_activity_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/admin_room_activity_avatar_view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:scaleType="centerCrop"
        android:layout_centerHorizontal="true" />

    <ImageView
        android:id="@+id/admin_room_activity_no_avatar_view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:visibility="gone"
        android:scaleType="fitCenter"
        android:src="@drawable/edit_avatar"
        android:layout_centerHorizontal="true"
        app:layout_marginTopPercent="14.99%"
        app:layout_aspectRatio="107%"
        app:layout_widthPercent="20%" />

    <androidx.percentlayout.widget.PercentRelativeLayout
        android:id="@+id/admin_room_activity_content_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/show_contact_content">

        <View
            android:id="@+id/admin_room_activity_slide_mark_view"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            app:layout_widthPercent="12%" />

        <androidx.percentlayout.widget.PercentRelativeLayout
            android:id="@+id/admin_room_activity_content_header_view"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:id="@+id/admin_room_activity_title_view"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:gravity="center"
                android:lines="1"
                android:text="@string/show_room_activity_room_title"
                app:layout_widthPercent="56%" />

        </androidx.percentlayout.widget.PercentRelativeLayout>

        <androidx.percentlayout.widget.PercentRelativeLayout
            android:id="@+id/admin_room_activity_name_content_view"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_below="@+id/admin_room_activity_content_header_view">

            <EditText
                android:id="@+id/admin_room_activity_name_view"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_gravity="left"
                android:background="@android:color/transparent"
                android:hint="@string/application_name_hint"
                android:imeOptions="actionDone"
                android:inputType="text|textNoSuggestions|textCapSentences"
                app:layout_marginStartPercent="5.31%"
                app:layout_marginEndPercent="5.31%" />

        </androidx.percentlayout.widget.PercentRelativeLayout>

        <TextView
            android:id="@+id/admin_room_activity_counter_name_view"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/admin_room_activity_name_content_view"
            android:layout_alignEnd="@+id/admin_room_activity_name_content_view"
            android:layout_alignRight="@+id/admin_room_activity_name_content_view" />

        <androidx.percentlayout.widget.PercentRelativeLayout
            android:id="@+id/admin_room_activity_description_content_view"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_below="@+id/admin_room_activity_name_content_view">

            <EditText
                android:id="@+id/admin_room_activity_description_view"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:hint="@string/admin_room_activity_welcome_message"
                android:background="@android:color/transparent"
                android:gravity="top"
                android:inputType="textMultiLine|textNoSuggestions|textCapSentences"
                app:layout_marginStartPercent="5.31%"
                app:layout_marginEndPercent="5.31%"
                app:layout_marginTopPercent="4.92%"
                app:layout_marginBottomPercent="4.92%" />
        </androidx.percentlayout.widget.PercentRelativeLayout>

        <TextView
            android:id="@+id/admin_room_activity_counter_description_view"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/admin_room_activity_description_content_view"
            android:layout_alignEnd="@+id/admin_room_activity_description_content_view"
            android:layout_alignRight="@+id/admin_room_activity_description_content_view" />

        <TextView
            android:id="@+id/admin_room_activity_configuration_title_view"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/admin_room_activity_counter_description_view"
            android:text="@string/application_configuration"
            android:layout_alignLeft="@+id/admin_room_activity_description_content_view"
            android:textAllCaps="true"
            android:maxLines="1" />

        <androidx.percentlayout.widget.PercentRelativeLayout
            android:id="@+id/admin_room_activity_settings_view"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_below="@+id/admin_room_activity_configuration_title_view"
            android:background="@drawable/item_light_border">

            <ImageView
                android:id="@+id/admin_room_activity_settings_image_view"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:src="@drawable/settings_icon"
                app:layout_aspectRatio="100%"
                app:layout_heightPercent="29.17%" />

            <TextView
                android:id="@+id/admin_room_activity_settings_text_view"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:text="@string/navigation_activity_settings"
                app:layout_marginEndPercent="4.27%"
                app:layout_marginStartPercent="12.8%" />

            <ImageView
                android:id="@+id/admin_room_activity_permissions_accessory_view"
                android:layout_centerVertical="true"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:scaleType="fitCenter"
                android:src="@drawable/end_chevron"
                app:tint="@color/accessory_color"
                app:layout_aspectRatio="54.55%"
                app:layout_heightPercent="25%" />
        </androidx.percentlayout.widget.PercentRelativeLayout>

        <androidx.percentlayout.widget.PercentRelativeLayout
            android:id="@+id/admin_room_activity_invite_view"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_below="@+id/admin_room_activity_settings_view"
            android:background="@drawable/item_light_border">

            <ImageView
                android:id="@+id/admin_room_activity_invite_image_view"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:src="@drawable/tab_bar_contacts_icon"
                app:layout_aspectRatio="100%"
                app:layout_heightPercent="29.17%" />

            <TextView
                android:id="@+id/admin_room_activity_invite_text_view"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:text="@string/show_room_activity_invite_participants"
                app:layout_marginEndPercent="4.27%"
                app:layout_marginStartPercent="12.8%" />

            <ImageView
                android:id="@+id/admin_room_activity_invite_accessory_view"
                android:layout_centerVertical="true"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:scaleType="fitCenter"
                android:src="@drawable/end_chevron"
                app:tint="@color/accessory_color"
                app:layout_aspectRatio="54.55%"
                app:layout_heightPercent="25%" />
        </androidx.percentlayout.widget.PercentRelativeLayout>

        <androidx.percentlayout.widget.PercentRelativeLayout
            android:id="@+id/admin_room_activity_code_view"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_below="@+id/admin_room_activity_invite_view"
            android:background="@drawable/item_light_border">

            <ImageView
                android:id="@+id/admin_room_activity_code_image_view"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:src="@drawable/qrcode"
                app:layout_aspectRatio="100%"
                app:layout_heightPercent="29.17%" />

            <TextView
                android:id="@+id/admin_room_activity_code_text_view"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:text="@string/profile_fragment_twincode_title"
                app:layout_marginEndPercent="4.27%"
                app:layout_marginStartPercent="12.8%" />

            <ImageView
                android:id="@+id/admin_room_activity_code_accessory_view"
                android:layout_centerVertical="true"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:scaleType="fitCenter"
                android:src="@drawable/end_chevron"
                app:tint="@color/accessory_color"
                app:layout_aspectRatio="54.55%"
                app:layout_heightPercent="25%" />
        </androidx.percentlayout.widget.PercentRelativeLayout>

        <androidx.percentlayout.widget.PercentRelativeLayout
            android:id="@+id/admin_room_activity_save_view"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_centerHorizontal="true"
            android:layout_below="@+id/admin_room_activity_code_view">

            <TextView
                android:id="@+id/admin_room_activity_save_title_view"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_centerInParent="true"
                android:gravity="center"
                android:text="@string/application_save"
                app:layout_widthPercent="89.06%" />

        </androidx.percentlayout.widget.PercentRelativeLayout>

        <androidx.percentlayout.widget.PercentRelativeLayout
            android:id="@+id/admin_room_activity_remove_view"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_below="@+id/admin_room_activity_save_view">

            <TextView
                android:id="@+id/admin_room__activity_remove_text_view"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:text="@string/application_delete" />
        </androidx.percentlayout.widget.PercentRelativeLayout>

    </androidx.percentlayout.widget.PercentRelativeLayout>

    <androidx.percentlayout.widget.PercentRelativeLayout
        android:id="@+id/admin_room_activity_back_clickable_view"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:clickable="true"
        android:orientation="horizontal"
        app:layout_aspectRatio="100%">

        <org.twinlife.twinme.utils.RoundedView
            android:id="@+id/admin_room_activity_back_rounded_view"
            android:layout_width="match_parent"
            android:layout_height="match_parent" />

        <ImageView
            android:id="@+id/admin_room_activity_back_image_view"
            android:layout_centerInParent="true"
            android:scaleType="fitCenter"
            android:src="@drawable/start_chevron"
            app:layout_aspectRatio="62.5%"
            app:layout_heightPercent="37.5%" />

    </androidx.percentlayout.widget.PercentRelativeLayout>

    <ProgressBar
        android:id="@+id/admin_room_activity_progress_bar"
        style="@android:style/Widget.ProgressBar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true"
        android:visibility="gone" />

</androidx.percentlayout.widget.PercentRelativeLayout>